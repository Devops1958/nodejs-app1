pipeline{

    agent any
    stages{
        stage('Dep install'){
            agent { docker { image 'node:14'
               args '-u root:sudo'}}
            steps{
                
                // sh 'npm cache clean'
                sh 'apt install sudo -y'
             //   sh 'sudo chown -R 995:993 "/.npm"'
                sh 'npm install'
            }
        }
    }
}